<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="720.0" prefWidth="1080.0"
           styleClass="root-pane"
           stylesheets="@styles.css"
           xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.example.fpoe_50zo.controller.StartScreenController">
    <children>
        <VBox alignment="CENTER" spacing="50.0">
            <Label text="CINCUENTAZO" styleClass="title-label" />
            <Button fx:id="playButton" mnemonicParsing="false"
                    onAction="#onJugarClick" text="Jugar" />
        </VBox>
    </children>
</StackPane>